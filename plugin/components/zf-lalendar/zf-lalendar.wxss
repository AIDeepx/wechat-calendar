.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  /* padding: 200rpx 0; */
  box-sizing: border-box;
  color: #393a50;
} 


.zf-inner{
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
}
.zf-hearder{
  width: 100%;
  overflow: hidden;
  padding-left: 60rpx;
}
.zf-header-h5{
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  color: #222222;
}
.mgt20{
  margin-top: 20rpx;
}
.mgt24{
  margin-top: 24rpx;
}


.calendar{
  width: 100%;
  position: relative;
}
.calendar-header{
  background-color: #ffffff;
}
.calendar-inner{
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
}
.calendar-header-l,.calendar-header-r,.calendar-header-t{
  height: 100rpx;
  line-height: 100rpx;
}
.calendar-header-l,.calendar-header-r{
  width: 100rpx;
  text-align: center;
  color: #999999;
}
.calendar-header-t{
  flex: 1;
  text-align: center;
  font-size: 32rpx;
}
.calendar-mounth-title{
  position: relative;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  padding: 0 40rpx;
}
.calendar-mounth-title::after{
  width: 100%;
  height: 1px;
  content: "";
  background-color: #efefef;
  position: absolute;
  bottom: 0;
  left: 0;
}
.calendar-content,.calendar-content_amn{
  padding: 20rpx;
  font-size: 24rpx;
  margin-top: 1px;
  box-sizing: border-box;
  background-color: #ffffff;
}
.calendar-content_amn{
  position: absolute;
  width: 100%;
  top: 161rpx;
  left: 0;
  z-index: 999;
}

.calendar-content-item{
  position: relative;
  width: 80rpx;
  height: 80rpx;
  font-size: 30rpx;
  line-height: 75rpx;
  text-align: center;
  border-radius: 100%;
}
.calendar-content-item-active{
  color: #ffffff;
  background-color: #4788f4;
	box-shadow: 4rpx 6rpx 18rpx 0
		rgba(15, 76, 179, 0.21);
}
.calendar-content-item-desc,.calendar-content-item-desc-full{
  display: block;
  position: absolute;
  width: 100%;
  bottom: -20rpx;
  left: 0;
  font-size: 18rpx;
  font-weight: normal;
  color: #4788f4;
}

.calendar-content-item-desc-full{
  color: #ec4a85;
}
.calendar-content-item-active .calendar-content-item-desc,.calendar-content-item-active .calendar-content-item-desc-full{
  color: #ffffff;
  font-size: 60rpx;
  bottom: -5rpx;
}
.calendar-content-disable{
  color: #999999;
}
.calendar-content-month{
  width: 100%;
  position: absolute;
  top: 80rpx;
  left: 0;
}
.calendar-content-month .calendar-content-item{
  width: 100rpx;
  height: 150rpx;
}